{
    "$id": "http://xarxa.com/schemas/stockReference.json#",
    "title": "Stock Reference",
    "description": "A stock Reference",
    "type": [
        "object"
    ],
    "properties": {
        "quantity": {
            "type": "integer"
        },
        "reference": {
            "type": "string"
        },
        "releaseQuantity": {
            "type": "integer",
            "minimum": 1,
            "maximum": {
              "$data": "1/quantity"
            }
        }
    },
    "required": [
        "reference",
        "quantity"
    ]
}